package com.tanaya.flightreservation.entities;

import javax.persistence.Id;

public class AbstractEntity {
	
	@Id
	//@GeneratedValue(strategy=GenerationType.AUTO) 	//Only to make field auto-incremented in DB
	private long id;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

}
